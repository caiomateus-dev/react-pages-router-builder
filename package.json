{
  "name": "react-pages-router-builder",
  "version": "1.0.2",
  "description": "File-system based routing for React Router, inspired by Next.js App Router",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "bin": {
    "react-pages-router-builder": "./dist/cli.js"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-router": ">=6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "react",
    "router",
    "file-system",
    "nextjs",
    "routing",
    "file-based-routing",
    "cli",
    "generator",
    "pages"
  ],
  "author": "Caio Mateus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caiomateus-dev/react-pages-router-builder.git"
  },
  "bugs": {
    "url": "https://github.com/caiomateus-dev/react-pages-router-builder/issues"
  },
  "homepage": "https://github.com/caiomateus-dev/react-pages-router-builder#readme"
}
